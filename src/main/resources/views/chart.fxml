<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="mainContainer" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chart.main.ChartView">
   <children>
      <HBox fx:id="chartContainer" VBox.vgrow="ALWAYS" />
      <HBox fx:id="controlsContainer" alignment="CENTER_RIGHT" maxHeight="-Infinity" prefHeight="50.0" VBox.vgrow="NEVER">
         <children>
            <Button fx:id="addSeriesButton" mnemonicParsing="false" text="Add Series" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
