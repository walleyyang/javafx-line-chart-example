<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="mainContainer" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chart.main.ChartView">
   <children>
      <VBox fx:id="sliderContainer" prefHeight="50.0" VBox.vgrow="NEVER">
         <children>
            <HBox maxHeight="-Infinity" minHeight="-Infinity" prefHeight="20.0" VBox.vgrow="NEVER">
               <children>
                  <Label fx:id="lowRangeLabel" maxWidth="-Infinity" minHeight="-Infinity" prefWidth="100.0" />
                  <HBox fx:id="sliderLowRangeContainer" maxHeight="-Infinity" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <HBox maxHeight="-Infinity" minHeight="-Infinity" prefHeight="20.0" VBox.vgrow="NEVER">
               <children>
                  <Label fx:id="highRangeLabel" maxWidth="-Infinity" minHeight="-Infinity" prefWidth="100.0" />
                  <HBox fx:id="sliderHighRangeContainer" />
               </children>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </HBox>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </VBox>
      <HBox fx:id="chartContainer" VBox.vgrow="ALWAYS" />
      <HBox fx:id="controlsContainer" alignment="CENTER_RIGHT" maxHeight="-Infinity" prefHeight="50.0" VBox.vgrow="NEVER">
         <children>
            <Button fx:id="addSeriesButton" mnemonicParsing="false" text="Add Series" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
